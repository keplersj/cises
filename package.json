{
  "name": "cises",
  "description": "A browser extension that incrementally improves on the University of Utah CIS experience.",
  "version": "0.0.0-dev",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Kepler Sticka-Jones",
    "email": "kepler@stickajones.org",
    "url": "https://keplersj.org"
  },
  "scripts": {
    "clean": "rimraf build coverage cis-enhancement-suite_chrome.zip cis-enhancement-suite_firefox.xpi",
    "build": "webpack",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js, jsx, ts, tsx, json, css, md}": [
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "jest": {
    "automock": false,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "text"
    ],
    "setupFiles": [
      "./setupJest.ts"
    ],
    "snapshotSerializers": [
      "jest-emotion"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen"
  },
  "release": {
    "verifyConditions": [
      "semantic-release-chrome",
      "@semantic-release/github"
    ],
    "prepare": [
      {
        "path": "semantic-release-chrome",
        "asset": "cis-enhancement-suite_chrome.zip",
        "distFolder": "build"
      }
    ],
    "publish": [
      {
        "path": "semantic-release-chrome",
        "asset": "cis-enhancement-suite_chrome.zip",
        "extensionId": "pebfiklbcoioidmjoelliojmfamipfnn"
      },
      {
        "path": "@semantic-release/github",
        "assets": [
          {
            "path": "cis-enhancement-suite_chrome.zip"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "emotion": "^10.0.27",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/travis-cli": "^8.3.5",
    "@stryker-mutator/core": "^1.3.1",
    "@stryker-mutator/html-reporter": "^1.3.1",
    "@stryker-mutator/jest-runner": "^1.3.1",
    "@stryker-mutator/typescript": "^1.3.1",
    "@types/jest": "^25.1.2",
    "@types/pify": "^3.0.2",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^25.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-emotion": "^10.0.27",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-fetch-mock": "^3.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.2",
    "semantic-release-chrome": "^1.1.3",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.7.5",
    "web-ext-types": "^3.2.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
