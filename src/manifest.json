{
  "manifest_version": 2,
  "name": "CIS Enhancement Suite",
  "short_name": "CISes",
  "version": "0.0.0-dev",
  "description": "A browser extension that incrementally improves on the University of Utah CIS experience.",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "https://search-production.ratemyprofessors.com/",
    "webRequest"
  ],
  "content_scripts": [
    {
      "matches": ["https://utah.collegescheduler.com/*"],
      "js": ["college-scheduler.js"]
    }
  ]
}
